{
    "name": "mnoskov/evocms-minify",
    "description": "Minify css, less and js files in evo 3.x",
    "require": {
        "php": ">=7.3",
        "tedivm/jshrink": "~1.0",
        "matthiasmullie/minify": "^1.3",
        "scssphp/scssphp": "^1.6.0",
        "wikimedia/less.php": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "EvolutionCMS\\Minify\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EvolutionCMS\\Minify\\MinifyServiceProvider"
            ]
        }
    }
}
